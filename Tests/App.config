<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections> 
    <sectionGroup name="Browsers">
      <section name="Chrome" type="System.Configuration.NameValueSectionHandler" />
      <section name="IE11" type="System.Configuration.NameValueSectionHandler" />
      <section name="Firefox" type="System.Configuration.NameValueSectionHandler" />
      <section name="Edge" type="System.Configuration.NameValueSectionHandler" />
      <section name="AndroidChromeNexus4" type="System.Configuration.NameValueSectionHandler" />
      <section name="AndroidChromeNexus6" type="System.Configuration.NameValueSectionHandler" />
      <section name="Safari" type="System.Configuration.NameValueSectionHandler" />
      <section name="SafariIphone7" type="System.Configuration.NameValueSectionHandler" />
      <section name="SafariIphone7Plus" type="System.Configuration.NameValueSectionHandler" />
    </sectionGroup>
  
  
  </configSections>
  <Browsers>
    <Chrome>
      <add key="browserName" value="chrome" />
      <add key="platform" value="" />
    </Chrome>
    <IE11>
      <add key="browserName" value="internet explorer" />
      <add key="platform" value="" />
    </IE11>
    <Firefox>
      <add key="browserName" value="firefox" />
      <add key="platform" value="" />
      <add key="version" value="" />
    </Firefox>
    <Edge>
      <add key="browserName" value="MicrosoftEdge" />
      <add key="platform" value="" />
    </Edge>
    <Safari>
      <add key="browserName" value="safari" />
      <add key="platformName" value="" />
    </Safari>
    <SafariIphone7>
      <add key="browserName" value="safariIphone" />
      <add key="platformName" value="iOS" />
      <add key="platformVersion" value="10.12" />
      <add key="deviceName" value="iPhone 7" />
      <add key="automationName" value="XCUITest" />
    </SafariIphone7>
    <SafariIphone7Plus>
      <add key="browserName" value="safariIphone" />
      <add key="platformName" value="iOS" />
      <add key="platformVersion" value="10.3" />
      <add key="deviceName" value="iPhone 7 Plus" />
      <add key="automationName" value="XCUITest" />
    </SafariIphone7Plus>
    <AndroidChromeNexus4>
      <add key="browserName" value="androidchrome" />
      <add key="device" value="Android" />
      <add key="platformName" value="Android" />
      <add key="deviceName" value="Nexus4API23Chrome55_API_23" />
      <add key="newCommandTimeout" value="580" />
    </AndroidChromeNexus4>
    <AndroidChromeNexus6>
      <add key="browserName" value="androidchrome" />
      <add key="device" value="Android" />
      <add key="platformName" value="Android" />
      <add key="deviceName" value="Nexus_6_API_26" />
      <add key="newCommandTimeout" value="580" />
    </AndroidChromeNexus6>
  </Browsers>
  
  <appSettings>
    <add key="RunAllTestsOn" value="Chrome" />
    <add key="Hub" value="http://localhost:4444/wd/hub" />
    <add key="Appium" value="" />
    <add key="AppURI" value="https://mondo.com.au/" />
  </appSettings>
  

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" /></startup></configuration>
